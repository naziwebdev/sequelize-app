{
  "name": "sequelize-app",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "./dist/server.js",
  "scripts": {
    "ts:node": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node build/src",
    "dev": "dotenv -e .env nodemon ./dist/server.js",
    "db": "dotenv -e .env -- sequelize-cli db:create --config ./dist/sequelizeConfigs.js",
    "migrate": "dotenv -e .env -- sequelize-cli db:migrate --config ./dist/sequelizeConfigs.js "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "svg-captcha": "^1.4.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sequelize": "^4.28.20",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
